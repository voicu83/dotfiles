URxvt.font: xft:terminus:size=10

URxvt.scrollBar: false

! Tango color palette
URxvt*background:         #000000
URxvt*foreground:         #aaaaaa
URxvt*cursorColor:        #8ae234
! foreground color for underline
!URxvt*colorUL:            #8ae234
! line color for underline
!URxvt*underlineColor:     #92659a
! black dark/light
URxvt*color0:             #2e3436
URxvt*color8:             #6e706b
! red dark/light
URxvt*color1:             #cc0000
URxvt*color9:             #ef2929
! green dark/light
URxvt*color2:             #4e9a06
URxvt*color10:            #8ae234
! yellow dark/light
URxvt*color3:             #edd400
URxvt*color11:            #fce94f
! blue dark/light
URxvt*color4:             #3465a4
URxvt*color12:            #729fcf
! magenta dark/light
URxvt*color5:             #92659a
URxvt*color13:            #c19fbe
! cyan dark/light
URxvt*color6:             #07c7ca
URxvt*color14:            #63e9e9
! white dark/light
URxvt*color7:             #d3d7cf
URxvt*color15:            #eeeeec

! Perl extension config
URxvt.perl-ext-common: default,selection-to-clipboard,tabbed,vtwheel
! Any scripts placed here will override global ones with the same name
URxvt.perl-lib: /home/boggy/.urxvt/

! Tabbed extension configuration
!URxvt.tabbed.tabbar-fg: 8
!URxvt.tabbed.tabbar-bg: 0
!URxvt.tabbed.tab-fg:    15
!URxvt.tabbed.tab-bg:    8
URxvt.tabbed.new-button: false

! Tabbedex colors
URxvt.tabbed.tabbar-fg:   244
URxvt.tabbed.tabbar-bg:   16
URxvt.tabbed.tab-fg:      255
URxvt.tabbed.tab-bg:      52
URxvt.tabbed.bell-tab-fg: 255
URxvt.tabbed.bell-tab-bg: 160
URxvt.tabbed.bell-fg:     244
URxvt.tabbed.bell-bg:     17
URxvt.tabbed.title-fg:    28
URxvt.tabbed.title-bg:    16
